    // material module
    {
      "type": "Esp.VisGen2.Material.Infrastructure.MaterialDomainModule, Esp.VisGen2.Material.Infrastructure",
      "properties": {
        "MongoDbConnectionString": "mongodb://localhost:27017",
        "MaterialDatabaseName": "VisGen2",
